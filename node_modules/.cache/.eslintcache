utton';\r\nconst Product = ({bike}) => {\r\n    const navigate = useNavigate();\r\n    const dispatch = useDispatch();\r\n\r\n    // Открытие товара\r\n    const openProductViewBike = (event) =>{\r\n    event.stopPropagation();\r\n    dispatch(setCurrentBike(bike))\r\n    navigate(`/bikes/${bike.id}`); \r\n  }\r\n    return (\r\n        <div className='product-card'>\r\n            <div>\r\n            <img onClick={openProductViewBike} className='product-bmx-image' src={bike.img} alt=\"\" />\r\n            <h2>{bike.name}</h2>\r\n            <p>{bike.description}</p>\r\n            </div>\r\n            <div style={{marginTop:'15px'}}>\r\n                <h3 className='product-card-price'>{bike.price}$</h3>\r\n                <AddAndDeleteToCartButton bike={bike}/>\r\n            </div>\r\n            \r\n           \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Product","D:\\ReactProjects\\react-ecommerce-shop\\src\\components\\CartProducts.jsx",["189","190"],[],"import React, { useContext } from 'react'\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport deleteBin from '../img/delete-bin.png'\r\nimport { deleteItemFromCart } from '../redux/cart/reducer';\r\nimport { useNavigate } from 'react-router-dom';\r\n\r\nconst CartProducts = ({setOpenCart, bikeItems }) => {\r\n  const navigate = useNavigate();\r\n  \r\n  // Цена\r\n  const totalPrice = bikeItems.reduce((acc, bike) => acc += bike.price, 0);\r\n  \r\n  const dispatch = useDispatch();\r\n\r\n\r\n  const goOrder = (event) => \r\n  {\r\n    event.preventDefault();\r\n    setOpenCart(false);//закрытие корзины по перехожу на страницу заказа\r\n    navigate('/order');\r\n  }\r\n  return (\r\n    <div>\r\n      {bikeItems.map(bike =><div key={bike.id} className='productInCart-block'>\r\n        <div className='productInCart-information'>\r\n          <img src={bike.img} alt=\"\" width={100}/> \r\n          <div> \r\n            <h2 className='productInCart-name'>{bike.name}</h2>\r\n            <h3 className='productInCart-price'>{bike.price}$</h3>\r\n          </div>\r\n           \r\n        </div>\r\n        <div ><img onClick={() => dispatch(deleteItemFromCart(bike.id))} className='deleteProduct-icon' src={deleteBin} alt=\"\" /></div>  \r\n        </div>\r\n        )}\r\n      {bikeItems.length > 0 ?\r\n        <div>\r\n          \r\n          <h2>Total Price: {totalPrice}</h2>\r\n          <button className='orderButton' onClick={goOrder}>Оформить заказ</button>\r\n        </div>\r\n        : <h2>Коризна пуста!</h2>}\r\n\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default CartProducts;","D:\\ReactProjects\\react-ecommerce-shop\\src\\context\\index.js",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\bikes.js",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\components\\Content.jsx",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\pages\\Main.jsx",["191"],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\components\\Cart.jsx",["192"],[],"import React, { useContext, useState } from 'react'\r\nimport CartProducts from './CartProducts';\r\nimport cart from '../img/cart.png'\r\nimport { useSelector } from 'react-redux';\r\nimport CartAmountProducts from './CartAmountProducts';\r\nimport { AuthContext } from '../context';\r\n\r\nconst Cart = () => {\r\n    const bikeItems = useSelector(state => state.cart.itemsInCart);\r\n  \r\n    const {isOpenCart,setIsOpenCabinet}= useContext(AuthContext);\r\n\r\n    // Закрытие мой аккаунт если открыли корзину\r\n  const {setOpenCart}= useContext(AuthContext);\r\n    const openAndCloseCart = () => {\r\n        setOpenCart(!isOpenCart);\r\n        setIsOpenCabinet(false);\r\n    }\r\n    return (\r\n        <div className='cart-block'>\r\n            <div className='cart' onClick={openAndCloseCart}>\r\n                {/*Число товаров в корзине*/}\r\n                {bikeItems.length > 0 && <CartAmountProducts quantity={bikeItems.length} />} \r\n                <li  className={`navigation-item ${isOpenCart && 'activeCart'}`}><img className='cart-ico' src={cart} alt=\"\" /></li>\r\n            </div>\r\n            <div className='openCart'>\r\n                {isOpenCart && (<div className='shopCart'><span onClick={() => setOpenCart(false)} className='shopCart-close'>X</span><CartProducts setOpenCart={setOpenCart} bikeItems={bikeItems}/></div>)}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Cart","D:\\ReactProjects\\react-ecommerce-shop\\src\\components\\MyButton\\MyButton.jsx",["193","194"],[],"import React, { useContext } from 'react'\r\nimport { AuthContext } from '../../context';\r\nimport '../MyButton/MyButton.css'\r\n\r\nconst MyButton = ({addToCartAndDelete,children,type}) => {\r\n  \r\n  return (\r\n    <div className='myBytton-block'>\r\n      <button onClick={addToCartAndDelete} className={type}>{children}</button>\r\n    </div>\r\n    \r\n  )\r\n}\r\n\r\nexport default MyButton;","D:\\ReactProjects\\react-ecommerce-shop\\src\\redux\\store.js",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\redux\\cart\\reducer.js",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\components\\CartAmountProducts.jsx",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\redux\\bikes\\reducer.js",[],[],"D:\\ReactProjects\\react-ecommerce-shop\\src\\components\\ProductView.jsx",["195"],[],"import React from 'react'\r\nimport { useSelector } from 'react-redux'\r\nimport AddAndDeleteToCartButton from './AddAndDeleteToCartButton'\r\nimport MyButton from './MyButton/MyButton'\r\nconst ProductView = () => {\r\n  const bike = useSelector(state => state.bike.currentBike)\r\n  if(!bike) return null\r\n  return (\r\n    <div className='productView-page'>\r\n        <img src={bike.img} alt=\"\" width={600}/>\r\n        <div className='productView-information'>\r\n            <h1 className='productView-title'>{bike.name}</h1>\r\n            <h2 className='productView-price'>{bike.price}$</h2>\r\n            <p className='productView-details'>{bike.detail